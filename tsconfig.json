{
  "compilerOptions": {
    "strict": true,
    "module": "ES2020",
    "moduleResolution": "NodeNext",
    "target": "ES2020",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "types": [
      "vite/client"
    ],
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "plugins": [
      {
        "name": "typescript-plugin-css-modules"
      }
    ]
  }
}
